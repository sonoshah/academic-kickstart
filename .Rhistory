library(knitr)
library(kableExtra)
library(tidyverse)
dta <- tibble::tribble(
~`GOP Share House Contributions`,  ~`2014`, ~`2018`, ~Change,
"Asian Contributions",  .24, .34,   .10,
"All Individual Contributions", .58, .54,   -.04
)
dta2 <- tibble::tribble(
~` `, ~`GOP Share Asian Contributions`, ~`Change from 2014`,
"All Asian Contributions",                                    .34,                .10,
"All Asian Contributions (w/out Kim)",                                   .29,                 .05
)
dta2 %>%
mutate(`GOP Share Asian Contributions` = scales::percent(`GOP Share Asian Contributions`,2),
`Change from 2014` = scales::percent(`Change from 2014`,2)) %>%
mutate(Change = cell_spec(Change, "html", color = ifelse(Change >0, "green", "red"))) %>%
kable(dta, format = "html", escape = F) %>%
kable_styling(full_width = T,
bootstrap_options = c("striped", "hover", "responsive"))
